    "one_genre = \"\"\"\n",
    "WITH pure_genre \n",
    "AS\n",
    "    (\n",
    "    SELECT \n",
    "    genres\n",
    "    FROM films\n",
    "    WHERE genres NOT LIKE '%,%'\n",
    "    ),\n",
    "\n",
    "non_pop \n",
    "AS\n",
    "    (\n",
    "    SELECT\n",
    "    genres, \n",
    "    count(genres) count,\n",
    "    cast(count(genres) AS FLOAT) / (\n",
    "        SELECT COUNT(*) from pure_genre\n",
    "    ) * 100 total_percent\n",
    "    FROM pure_genre\n",
    "    GROUP BY genres\n",
    "    ),\n",
    "\n",
    "joined_genre \n",
    "AS\n",
    "    (\n",
    "    SELECT \n",
    "    genres\n",
    "    FROM films\n",
    "    INNER JOIN reviews\n",
    "    ON films.tconst = reviews.tconst\n",
    "    WHERE genres NOT LIKE '%,%'\n",
    "    ),\n",
    "\n",
    "pop_genre \n",
    "AS\n",
    "    (\n",
    "SELECT\n",
    "genres, \n",
    "count(genres) count,\n",
    "cast(count(genres) AS FLOAT) / (\n",
    "        SELECT COUNT(*) from joined_genre\n",
    "    ) * 100 pop_movie_percent\n",
    "FROM joined_genre\n",
    "GROUP BY genres\n",
    "    )\n",
    "\n",
    "SELECT \n",
    "non_pop.*,\n",
    "pop_genre.pop_movie_percent\n",
    "FROM non_pop\n",
    "INNER JOIN pop_genre\n",
    "ON non_pop.genres = pop_genre.genres\n",
    "ORDER BY 2 DESC\n",
    "LIMIT 6\n",
    ";    \n",
    "    \n",
    "\n",
    "\"\"\"\n",
    "\n",
    "run_query(one_genre)\n"
   ]
